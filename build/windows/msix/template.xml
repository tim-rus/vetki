<?xml version="1.0" encoding="utf-8"?>
<MsixPackagingToolTemplate
    xmlns="http://schemas.microsoft.com/msix/packaging/msixpackagingtool/template/2022">
    <Settings
        AllowTelemetry="false"
        ApplyACLsToPackageFiles="true"
        GenerateCommandLineFile="true"
        AllowPromptForPassword="false">
    </Settings>
    <Installer
        Path="vetki.exe"
        Arguments=""
        InstallLocation="C:\Program Files\Vetki\Vetki">
    </Installer>
    <PackageInformation
        PackageName="Vetki"
        PackageDisplayName="Vetki"
        PublisherName="CN=Vetki"
        PublisherDisplayName="Vetki"
        Version="0.1.0.0"
        PackageDescription="Vetki">
        <Capabilities>
            <Capability Name="runFullTrust" />
            
        </Capabilities>
        <Applications>
            <Application
                Id="com.wails.vetki"
                Description="Vetki"
                DisplayName="Vetki"
                ExecutableName="vetki.exe"
                EntryPoint="Windows.FullTrustApplication">
                
            </Application>
        </Applications>
        <Resources>
            <Resource Language="en-us" />
        </Resources>
        <Dependencies>
            <TargetDeviceFamily Name="Windows.Desktop" MinVersion="10.0.17763.0" MaxVersionTested="10.0.19041.0" />
        </Dependencies>
        <Properties>
            <Framework>false</Framework>
            <DisplayName>Vetki</DisplayName>
            <PublisherDisplayName>Vetki</PublisherDisplayName>
            <Description>Vetki</Description>
            <Logo>Assets\AppIcon.png</Logo>
        </Properties>
    </PackageInformation>
    <SaveLocation PackagePath="vetki.msix" />
    <PackageIntegrity>
        <CertificatePath></CertificatePath>
    </PackageIntegrity>
</MsixPackagingToolTemplate>
